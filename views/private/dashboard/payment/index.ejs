<div class ="container-fluid">
    <div class="row">

          <div class="col-md-2" >
            <% include ../../../partials/dashboard_left_panel %>
          </div>

        <div class="col-md-9">
              <table>
              <% Object.keys(output).forEach(function(key) { %>
                <tr>
                  <td> <%= deCamelCaser(key)%></td>
                  <td> <%= output[key] %> </td>
                </tr>
              <% }); %>
            </table>
        </div>
    </div>

    <div class="row">
       <div class="col-md-2 col-md-offset-2">
          <a class="btn btn-success" href="/dashboard/payment/add" role="button">
            Change Payment Information
          </a>
      </div>
    </div>
</div>
